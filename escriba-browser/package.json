{
  "name": "@mycloudlab/escriba-browser",
  "version": "1.0.0",
  "description": "Log library, for usage on browser and aggregate logs into server and loki dashboard",
  "main": "dist/escriba.min.js",
  "module": "dist/escriba.min.mjs",
  "typings": "dist/escriba.d.ts",
  "type": "module",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "rimraf dist && rollup -c",
    "watch": "tsc --watch",
    "test": "jest --config jestconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mycloudlab/escriba"
  },
  "keywords": [
    "server",
    "logging",
    "log",
    "browser",
    "grafana",
    "loki",
    "promtail",
    "gelf",
    "MDC"
  ],
  "author": {
    "name": "Carlos Alberto",
    "email": "euprogramador@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/mycloudlab/escriba/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "esbuild": "^0.16.4",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.7.3",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-uglify": "^5.0.2",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  },
  "files": [
    "dist/*",
    "README.md"
  ],
  "dependencies": {
    "stacktrace-js": "^2.0.2"
  }
}
