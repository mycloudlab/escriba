{
  "name": "@mycloudlab/escriba-browser",
  "version": "1.0.0",
  "description": "Log library, for usage on browser and aggregate logs into server and loki dashboard",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mycloudlab/jslog"
  },
  "keywords": [
    "server",
    "logging",
    "log",
    "browser"
  ],
  "author": {
    "name": "Carlos Alberto",
    "email": "euprogramador@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/mycloudlab/jslog/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "stacktrace-js": "^2.0.2"
  }
}
